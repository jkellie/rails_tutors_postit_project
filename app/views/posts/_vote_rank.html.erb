<h4>Rank: <%= counter %></h4>
<br />
<div class="btn-group">
	<%= link_to '<i class="icon-plus"></i>'.html_safe, voteup_post_path(post), class: "btn btn-mini" %>
	<%= link_to '<i class="icon-minus"></i>'.html_safe, votedown_post_path(post), class: "btn btn-mini" %>
</div>
<div id="total_votes_<%= post.id %>">
	<em>Votes: <%= post.vote_score %></em>
</div>